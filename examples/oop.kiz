object Point
	_x = 0
	_y = 0

	fn get_x(this)
		return this._x
	end

	fn get_y(this)
		return this._y
	end

	fn __call__(this, x, y)
		# 这里的create函数实际上是创造一个空对象, 其__parent__属性指向Point
		obj = create(this)
		obj._x = x
		obj._y = y
		return obj
	end

	fn __add__(this, other)
		return Point(this._x + other._x, this._y + other._y)
	end

	fn __str__(this)
		return "Point(x=" + this._x + ", y=" + this._y
	end
	
end

p = Point(1,2)
s = Point(3,4)

print(p)
print(p+s)
